class ShoppingCart {
    int total = 0;

    void addItem(int price) {
        total += price;
    }

    void showTotal() {
        System.out.println("Total Bill: " + total);
    }

    public static void main(String[] args) {
        ShoppingCart cart = new ShoppingCart();
        cart.addItem(500);
        cart.addItem(1200);
        cart.showTotal();
    }
}